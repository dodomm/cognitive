<?xml version="1.0"?>
<block>
  <name>three_ch_multip_rtdex</name>
  <key>avio505_three_ch_multip_rtdex</key>
  <category>avio505</category>
  <import>import avio505</import>
  <make>avio505.three_ch_multip_rtdex()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
 

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in_dme1</name>
    <type>complex</type>
  </sink>

  <sink>
    <name>in_dme2</name>
    <type>complex</type>
  </sink>

    <sink>
    <name>in_adsb</name>
    <type>complex</type>
  </sink>



  <source>
    <name>out</name>
    <type>short</type>
  </source>
</block>
